<mvc:View
xmlns="sap.m"
controllerName="sap.ui.demo.walkthrough.controllers.App"
xmlns:mvc="sap.ui.core.mvc"
displayBlock="true"
>
	<Shell>
	<App class="sapUiDemoWalkthrough">
		<pages>
			<Page title="{i18n>welcomePageTitle}">
				<headerContent>
					<Button
					id="helloDialogButton"
					icon="sap-icon://world"
					text="{i18n>showDialogButtonText}"
			    	press=".onOpenDialog"
			    	class="sapUiSmallMarginEnd"/>
				</headerContent>
				<content>
					<mvc:XMLView viewName="sap.ui.demo.walkthrough.views.HelloPanel" />
			    </content>
			    
	    	</Page>
	    </pages>
	</App>
	</Shell>
</mvc:View>