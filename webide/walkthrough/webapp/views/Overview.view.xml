<mvc:View
xmlns="sap.m"
controllerName="sap.ui.demo.walkthrough.controllers.App"
xmlns:mvc="sap.ui.core.mvc"
displayBlock="true"
>
	<Page title="{i18n>welcomePageTitle}">
		<headerContent>
			<Button
			icon="sap-icon://world"
	    	press=".onOpenDialog"
	    	class="sapUiSmallMarginEnd"/>
		</headerContent>
		<content>
			<mvc:XMLView viewName="sap.ui.demo.walkthrough.views.HelloPanel" />
			<mvc:XMLView viewName="sap.ui.demo.walkthrough.views.InvoiceList"/>
	    </content>
	    
	</Page>	
</mvc:View>