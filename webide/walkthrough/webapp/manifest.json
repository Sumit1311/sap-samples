{
	"_version": "1.12.0",
	"sap.app" : {
		"_version": "1.3.0",
		
		"id":"sap.ui.demo.walkthrough",
		"i18n": "i18n/i18n.properties",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}"
	},
	"sap.ui" : {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5" : {
		 "_version": "1.2.0",
		 "rootView": {
			"viewName": "sap.ui.demo.walkthrough.views.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
		  "minUI5Version": "1.60",
		  "libs": {
			"sap.m": {}
		  }
		},
		"models": {
		  "i18n": {
			"type": "sap.ui.model.resource.ResourceModel",
			"settings": {
			  "bundleName": "sap.ui.demo.walkthrough.i18n.i18n"
			}
		  }
		},
		"resources": {
			"css": [
				{
				  "uri": "css/style.css"
				}
			]
	}
	}
}