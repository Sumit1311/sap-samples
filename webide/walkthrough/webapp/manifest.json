{
	"_version": "1.12.0",
	"sap.app" : {
		"_version": "1.3.0",
		
		"id":"sap.ui.demo.walkthrough",
		"i18n": "i18n/i18n.properties",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources" : {
			"invoiceServiceRemote" :{
				"uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings":{"odataVersion": "2.0"}
			},
			"northwind": {
				"uri": "http://localhost:8081/https://services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			},
			"herokuProxy": {
				"uri": "https://cors-anywhere.herokuapp.com/https://services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui" : {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5" : {
		 "_version": "1.2.0",
		 "rootView": {
			"viewName": "sap.ui.demo.walkthrough.views.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
		  "minUI5Version": "1.60",
		  "libs": {
			"sap.m": {}
		  }
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
				  "bundleName": "sap.ui.demo.walkthrough.i18n.i18n"
				}
			},
			"invoice": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "localService/mockdata/Invoices.json"
			},
			"invoiceOData" : {
				"dataSource" : "invoiceServiceRemote"
			} 
		},
		"resources": {
			"css": [
				{
				  "uri": "css/style.css"
				}
			]
	}
	}
}